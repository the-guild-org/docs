{
  "name": "@theguild/components",
  "version": "5.2.2",
  "repository": {
    "url": "https://github.com/the-guild-org/docs",
    "directory": "packages/components"
  },
  "license": "MIT",
  "main": "./dist/index.js",
  "exports": {
    "./style.css": "./style.css",
    "./search-bar-v2.css": "./search-bar-v2.css",
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./next.config": {
      "import": "./dist/next.config.mjs",
      "types": "./dist/next.config.d.ts"
    },
    "./*": {
      "import": "./dist/*.js",
      "types": "./dist/*.d.ts"
    }
  },
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index.d.ts"
      ],
      "logos": [
        "./dist/logos.d.ts"
      ],
      "products": [
        "./dist/products.d.ts"
      ],
      "next.config": [
        "./dist/next.config.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "search-bar-v2.css",
    "style.css"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "prebuild": "pnpm types:check && rm -rf dist",
    "types:check": "tsc --noEmit"
  },
  "peerDependencies": {
    "next": "^12.3.1 || ^13.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "dependencies": {
    "@algolia/autocomplete-js": "1.11.0",
    "@algolia/autocomplete-plugin-algolia-insights": "1.11.0",
    "@algolia/autocomplete-plugin-query-suggestions": "1.11.0",
    "@algolia/autocomplete-theme-classic": "1.11.0",
    "@giscus/react": "2.3.0",
    "@next/bundle-analyzer": "13.4.2",
    "@radix-ui/react-navigation-menu": "1.1.3",
    "@theguild/remark-npm2yarn": "workspace:*",
    "algoliasearch": "4.19.1",
    "clsx": "2.0.0",
    "focus-trap-react": "10.2.1",
    "fuzzy": "^0.1.3",
    "next-videos": "1.5.0",
    "nextra": "2.9.0",
    "nextra-theme-docs": "2.9.0",
    "react-instantsearch-dom": "6.40.4",
    "react-paginate": "8.2.0",
    "react-player": "2.12.0",
    "remark-mdx-disable-explicit-jsx": "0.1.0",
    "search-insights": "2.7.0",
    "semver": "^7.3.8",
    "use-debounce": "9.0.4"
  },
  "devDependencies": {
    "@theguild/algolia": "workspace:*",
    "@theguild/editor": "workspace:*",
    "@types/dedent": "0.7.0",
    "@types/react-dom": "18.2.7",
    "@types/semver": "7.5.0",
    "dedent": "0.7.0",
    "esbuild-plugin-svgr": "1.1.0",
    "graphql": "16.8.0",
    "next": "13.4.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.3.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false
}
